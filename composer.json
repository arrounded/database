{
    "name": "arrounded/database",
    "description": "Database and Eloquent helpers",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Madewithlove",
            "email": "heroes@madewithlove.be"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/database": "^5.1"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "2.0.*@dev",
        "phpunit/phpunit": "^4.7",
        "mockery/mockery": "^0.9.4",
        "phpspec/phpspec": "^2.2",
        "symfony/var-dumper": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Arrounded\\Database\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arrounded\\Database\\": "tests"
        },
        "files": [
            "tests/helpers.php"
        ]
    },
    "scripts": {
        "test": "phpunit",
        "lint": "php-cs-fixer fix"
    }
}
